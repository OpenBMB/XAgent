  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:test": "npm run clean && vue-tsc &&cross-env type=test vite build && npm run zip:test",
    "build:prod": "npm run clean && vue-tsc && cross-env type=prod vite build && npm run zip:prod",
    "build:prod_docker": "npm run clean && vue-tsc && cross-env type=prod vite build",
    "clean": "rm -r -f dist $npm_package_name.test.zip $npm_package_name.zip",
    "zip:test": "zip -r -q $npm_package_name.test.zip dist",
    "zip:prod": "zip -r -q $npm_package_name.zip dist"
  },
